class Admin  {  
public function saludo() {
echo "Que tal buen hombre>> " . 
$this->usuario;
}
private function suspenso($usuario) {
echo $usuario->usuario . " su nota es menor que 5 por lo que has 	 supendido";
}
}
$admin = new Admin;
$admin->usuario = "Tu tia <BR>";
$admin->saludo(); 	
class Usuario {
public $usuario;
public $contraseña;
private function __construct() {
$this->contraseña = 'Maeton salvaje566';
}


public function saludo() {
echo 'Las notas no son las esperadas asi que tendremos que hablar 	  de ellas ' . 
$this->usuario ;

}
}
$Kaki = new Usuario;
$Kaki->usuario = 'Castro <BR>';
$Kaki->saludo();
class Perfil{
public $notas;
private static $NNotasExamenes = 1.56;
}
public static function siNota ($nota) {
}
if(strlen($nota) >= self::$NNotasExamenes){
return true;
}
}else{
return false
}
